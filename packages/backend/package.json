{
  "name": "backend",
  "version": "1.0.0",
  "description": "SpecGen AI Backend API - Serverless Express Application",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:watch": "webpack --mode development --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "sam:build": "npm run build && sam build",
    "sam:build:watch": "sam build --watch",
    "sam:deploy": "sam deploy",
    "sam:deploy:dev": "sam deploy --config-env dev",
    "sam:deploy:staging": "sam deploy --config-env staging",
    "sam:deploy:prod": "sam deploy --config-env prod",
    "sam:local": "sam local start-api --port 3001",
    "sam:local:invoke": "sam local invoke",
    "sam:sync": "sam sync --watch",
    "sam:delete": "sam delete",
    "deploy": "npm run sam:build && npm run sam:deploy",
    "deploy:dev": "npm run sam:build && npm run sam:deploy:dev",
    "deploy:staging": "npm run sam:build && npm run sam:deploy:staging",
    "deploy:prod": "npm run sam:build && npm run sam:deploy:prod",
    "local": "npm run sam:build && npm run sam:local",
    "remove": "npm run sam:delete"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.441.0",
    "@aws-sdk/client-dynamodb": "^3.441.0",
    "@aws-sdk/client-s3": "^3.441.0",
    "@aws-sdk/client-sfn": "^3.441.0",
    "@aws-sdk/lib-dynamodb": "^3.859.0",
    "aws-jwt-verify": "^4.0.1",
    "aws-sdk": "^2.1489.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-http-context": "^2.0.1",
    "express-winston": "^4.2.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "serverless-http": "^3.2.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@types/compression": "^1.7.4",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.7",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.8.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "serverless": "^3.36.0",
    "serverless-offline": "^13.2.0",
    "serverless-webpack": "^5.13.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
